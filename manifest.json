{
  "manifest_version": 2,
  "name": "Mobile Go",
  "version": "1.1.1",
  "description": "Finish your navigation on your smart phone!",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Mobile Go",
    "default_icon": "favicon_128.png"
  },
  "icons": {
    "16": "favicon_16.png",
    "48": "favicon_48.png",
    "128": "favicon_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
      "index.html",
      "assets/logo.png"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
          "https://*/*",
          "http://*/*",
          "*://*/*"
      ],
      "js": [
          "js/lib/jquery.min.js",
          "js/content-script.js"
      ],
      "css" : ["css/iframe.css"]
    }
  ]
}
